// NUMBER HASHING
#include <bits/stdc++.h>
using namespace std;

int main() {
    // Write C++ code here
    int i,j,n,m,num,ar[100];
    cin>>n;
    for(i=0;i<n;i++){
        cin>>ar[i];
    }
    int hash[13]={0};
    for(i=0;i<n;i++){
        hash[ar[i]]+=1;
    }
    cin>>m;
    while(m--){
        cin>>num;
        cout<<hash[num]<<endl;
    }
    return 0;
}
//using stl number hashing
// Online C++ compiler to run C++ program online
#include <bits/stdc++.h>
using namespace std;

int main() {
    // Write C++ code here
    int i,j,n,m,num,ar[100];
    cin>>n;                                //5
    for(i=0;i<n;i++){
        cin>>ar[i];                       // 1 2 1 2 1 
    }
    map<int,int> mpp;                    //it creates like a dictionary in python it maps (key,value) in mpp.
    for(i=0;i<n;i++){                    // mpp[key]=value.  you can access the key and value
        mpp[ar[i]]+=1;                   //ar[i] is key and the value is increseing by 1.
    }
    cin>>m;
    while(m--){
        cin>>num;
        cout<<mpp[num]<<endl;
    }
    for(auto it:mpp){
        cout<<it.first<<it.second<<endl;   // 1-->3  //2-->2
    }
    return 0;
}
// CHARACTER HASHING
#include <bits/stdc++.h>
using namespace std;
int main() {
    // Write C++ code here
    string s;
    char ch;
    int m;
    cin>>s;
    int hash[26]={0};
    for(char c=0;c<s.size();c++){
        hash[s[c]-'a']+=1;
    }
    
    cin>>m;
    while(m--){
        cin>>ch;
        cout<<hash[ch-'a'];
    }
    

    return 0;
}
